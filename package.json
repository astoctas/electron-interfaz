  },
  "dependencies": {
    "data-table": "^2.0.0",
    "electron-reload": "^1.4.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "johnny-five": "^1.1.0",
    "serialport": "^7.1.5",
    "socket.io": "^2.2.0",
    "vm2": "^3.8.1"
